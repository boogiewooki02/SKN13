    "def predict(model, x):\n",
    "    with torch.no_grad():\n",
    "        pred = model(x)\n",
    "        pred_proba = nn.Softmax(dim=-1)(pred)\n",
    "        result = torch.max(pred_proba, dim=-1)\n",
    "        return [(idx.item(), proba.item()) for idx, proba in zip(result.indices, result.values)]"
   ]
